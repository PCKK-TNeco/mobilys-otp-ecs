{
  "family": "otp-builder",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "2048",
  "memory": "12288",
  "executionRoleArn": "arn:aws:iam::892356203670:role/mobilys-otp-mobilys-staging-web-ExecutionRole-7cdEiSNAF8bV",
  "taskRoleArn": "arn:aws:iam::892356203670:role/mobilys-otp-mobilys-staging-web-TaskRole-GruyXEJmECzh",
  "containerDefinitions": [
    {
      "name": "builder",
      "image": "892356203670.dkr.ecr.ap-northeast-1.amazonaws.com/mobilys-otp/otp-builder:latest",
      "essential": true,
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/mobilys-otp/builder",
          "awslogs-region": "ap-northeast-1",
          "awslogs-stream-prefix": "builder"
        }
      }
    }
  ]
}
